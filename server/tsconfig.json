{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "outDir": "../dist",

    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "alwaysStrict": true,

    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    "noImplicitReturns": true,

    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {},
    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true,

    "jsx": "react"
  }
}
